{% set name  %}
    {{ cms }}
{% endset %}
{% extends "base.html.twig" %}
{% block title %}
    EpiHub - Articles : {{ property.title }}
{% endblock %}
{% block body %}
    <div class="container" style="margin-top: 5%; margin-bottom: 5%">
        <div class="card">
            <div class="card-header">
                <h3>{{ property.title }}</h3>
            </div>
            <div class="card-body">
                <small class="card-title">Créé par {{ property.author }}</small>
                <hr>
                <p> {{ property.description }} </p>
            </div>
        </div>
    </div>
{% endblock %}